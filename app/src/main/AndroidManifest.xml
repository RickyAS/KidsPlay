<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.testcom.kidscode">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_crown"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_crown_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Puzzle" />
        <activity android:name=".Quiz" />
        <activity android:name=".Arrange" />
        <activity android:name=".NameGuess" />
        <activity android:name=".Transition" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".GameOver" />
        <activity android:name=".Math" />
        <activity android:name=".EndGame"></activity>
        <service android:enabled="true" android:name=".SongService" />
    </application>

</manifest>